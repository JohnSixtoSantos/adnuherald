         <div class="main-panel"> 
           <div class="content">
                <div class="container-fluid">



                    <%= form_tag({controller: :data, action: :process_upload}, multipart: true) do %>
                    <div class="col-lg-12 col-md-6 col-sm-6">
                            <div class="card card-stats">
                                <div class="card-header card-header-warning card-header-icon">
                                    <div class="card-icon">
                                        <i class="material-icons">content_copy</i>
                                    </div>
                                    <p class="card-category">Dataset Upload</p>
                                    <h3 class="card-title">49/50
                                        <small>GB</small>
                                    </h3>
                                    <%= label_tag(:q, "Select a Tweet CSV file to upload:") %>
                                    <%= file_field_tag(:tweet_file) %>

                                    <%= label_tag(:q, "Collection Name:") %>
                                    <%= text_field_tag(:collection_name) %>  
                                     <%= submit_tag("Upload") %>
                                </div>
                                <div class="card-footer">
                                    <div class="stats">
                                       
                                    </div>
                                </div>
                            </div>
                    </div>
                    <% end %>


    

                    
                </div>
            </div> 
        </div>    