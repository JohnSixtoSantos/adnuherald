           <div class="content">
                <div class="container-fluid">
                    <div class="col-lg-12 col-md-12">
                            <div class="card">
                                <div class="card-header card-header-warning">
                                    <h4 class="card-title">Completed Centrality Analyses for Collection </h4>
                                    <p class="card-category">Below is a list of centrality analyses previously performed this collection.</p>
                                </div>
                                <div class="card-body table-responsive">
                                    <div id="keywords" style="height: 500px; width: 100%">
                                    </div>

                                    <div id="graph-container" style="width: 100%; height: 800px; display: block;">
                                    </div>

                                    <table class="table table-hover">
                                        <thead class="text-warning">
                                            <th>Twitter Usernames</th>
                                            <th>Centrality Score</th>
                                        </thead>
                                        <tbody>
                                            <% @user_hash.each do |k| %>
                                                <tr>
                                                    <td>
                                                        <%= k.text %>
                                                    </td>
                                                    <td>
                                                        <%= k.weight %>
                                                    </td>
                                                </tr>
                                            <% end %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>   
                </div>
            </div> 
            <script type="text/javascript">
                var words = <%= raw @words.to_json %>;


                $('#keywords').jQCloud(words, {
                      width: 900,
                      height: 450
                });

                var g = {
                    nodes: <%= raw @nodes.to_json %>,
                    edges: <%= raw @edges.to_json %>
                };


                // Instantiate sigma:
                s = new sigma({
                    graph: g,
                    container: 'graph-container',
                });

                config = {
                    gravity: true,
                    iterationsPerRender: 1
                };

                //s.startForceAtlas2();
            </script>